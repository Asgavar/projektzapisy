{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es6",
    "alwaysStrict": true,
    "strict": true,
    "strictNullChecks": false,
    "module": "commonjs",
    "baseUrl": "",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "*": [
        "/vagrant/zapisy/assets/*",
        "/webpack_modules/node_modules/*"
      ]
    },
    "typeRoots": [
      "/webpack_modules/node_modules/@types/",
      "./webpack_resources/typings/"
    ],
    "traceResolution": true
  },
  "include": [
    "assets/tsfix/index.ts"
  ]
}
