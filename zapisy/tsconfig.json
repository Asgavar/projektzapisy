{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es6",
    "alwaysStrict": true,
    "strict": true,
    "module": "commonjs",
    "baseUrl": "/",
    "paths": {
      "*": [
        "/vagrant/zapisy/assets/*",
        "/node/node_modules/*"
      ]
    },
    "typeRoots": [
      "/node/node_modules/@types",
      "/node/typings/"
    ],
    "traceResolution": true
  },
  "include": [
    "assets/tsfix/index.ts"
  ]
}